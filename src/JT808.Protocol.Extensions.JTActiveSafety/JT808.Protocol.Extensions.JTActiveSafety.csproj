    <Copyright>Copyright 2019.</Copyright>
    <Authors>SmallChi(Koike)</Authors>
    <PackageId>JT808.Protocol.Extensions.JTActiveSafety</PackageId>
    <Product>JT808.Protocol.Extensions.JTActiveSafety</Product>
